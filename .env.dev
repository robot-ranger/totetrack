# Environment file for development instance
# Use with: docker-compose --env-file .env.dev up -d

# Project name - this will be used to prefix containers and volumes
COMPOSE_PROJECT_NAME=totetrack-dev

# Database configuration
POSTGRES_DB=totetrack_dev
POSTGRES_USER=app
POSTGRES_PASSWORD=dev_password_123

# App settings
SECRET_KEY=dev_secret_key_not_for_production
ACCESS_TOKEN_EXPIRE_MINUTES=60
PASSWORD_RESET_TOKEN_EXPIRE_MINUTES=30
INITIAL_SUPERUSER_EMAIL=admin-dev@example.com
INITIAL_SUPERUSER_PASSWORD=dev_admin_password

# Port configuration - different ports to avoid conflicts
BACKEND_PORT=8882
FRONTEND_PORT=8890